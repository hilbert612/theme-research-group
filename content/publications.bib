@article{liu_strengthening_2024,
	title = {Strengthening {Near}‐{Infrared} {Photon} {Harvesting} in {Semi}‐{Transparent} {All}‐{Polymer} {Solar} {Cells} through the {Synergy} of {Fluorination} on the {Selenide} {Monomer} {Backbone}},
